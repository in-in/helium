{
	"name": "helium",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "gatsby build",
		"develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop --https --key-file $KEY_FILE_LOCALHOST --cert-file $CERT_FILE_LOCALHOST -H 0.0.0.0",
		"gen": "plop",
		"serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve -H 0.0.0.0",
		"start": "npm run develop"
	},
	"browserslist": [
		"last 2 versions and > 0.5%",
		"not ie <= 11",
		"not op_mini all",
		"not and_uc >= 12"
	],
	"dependencies": {
		"@danbruegge/gatsby-plugin-stylelint": "4.0.0",
		"gatsby": "2.18.21",
		"gatsby-plugin-manifest": "2.2.37",
		"gatsby-plugin-react-helmet": "3.1.21",
		"gatsby-plugin-react-svg": "3.0.0",
		"gatsby-plugin-robots-txt": "1.5.0",
		"gatsby-plugin-sass": "2.1.27",
		"gatsby-source-filesystem": "2.1.46",
		"gatsby-transformer-json": "2.2.25",
		"prop-types": "15.7.2",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-helmet": "5.2.1",
		"react-id-swiper": "2.4.0",
		"swiper": "5.3.0"
	},
	"devDependencies": {
		"@arkweid/lefthook": "0.7.0",
		"autoprefixer": "9.7.3",
		"babel-eslint": "10.0.3",
		"browserslist": "4.8.3",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "18.0.1",
		"eslint-plugin-import": "2.20.0",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.17.0",
		"eslint-plugin-react-hooks": "2.3.0",
		"plop": "2.5.3",
		"postcss-logical": "4.0.2",
		"postcss-normalize": "8.0.1",
		"sass": "1.24.4",
		"stylelint": "13.0.0",
		"stylelint-a11y": "1.2.2",
		"stylelint-order": "4.0.0",
		"stylelint-scss": "3.13.0",
		"stylelint-use-nesting": "1.2.1"
	}
}
