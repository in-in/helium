{
	"name": "helium",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "gatsby build",
		"develop": "NODE_TLS_REJECT_UNAUTHORIZED=0 gatsby develop --https --key-file $KEY_FILE_LOCALHOST --cert-file $CERT_FILE_LOCALHOST",
		"develop-m": "gatsby develop -H 0.0.0.0 -p 8000",
		"gen": "plop",
		"now-build": "npm run build",
		"serve": "gatsby serve",
		"start": "GATSBY_TELEMETRY_DISABLED=1 npm run develop"
	},
	"browserslist": [
		"last 2 versions and > 0.4%",
		"not ie <= 11",
		"not op_mini all"
	],
	"dependencies": {
		"apexcharts": "3.8.4",
		"autoprefixer": "9.6.1",
		"gatsby": "2.13.63",
		"gatsby-plugin-manifest": "2.2.5",
		"gatsby-plugin-react-helmet": "3.1.3",
		"gatsby-plugin-react-svg": "2.1.2",
		"gatsby-plugin-robots-txt": "1.5.0",
		"gatsby-plugin-sass": "2.1.8",
		"gatsby-source-filesystem": "2.1.9",
		"gatsby-transformer-json": "2.2.2",
		"node-sass": "4.12.0",
		"postcss-normalize": "8.0.1",
		"prop-types": "15.7.2",
		"react": "16.9.0",
		"react-apexcharts": "1.3.3",
		"react-dom": "16.9.0",
		"react-helmet": "5.2.1"
	},
	"devDependencies": {
		"@arkweid/lefthook": "0.6.3",
		"browserslist": "4.6.6",
		"eslint": "6.1.0",
		"eslint-config-airbnb": "18.0.1",
		"eslint-config-prettier": "6.0.0",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-react": "7.14.3",
		"eslint-plugin-react-hooks": "1.7.0",
		"plop": "2.4.0",
		"postcss-logical": "4.0.2",
		"prettier": "1.18.2",
		"stylelint": "10.1.0",
		"stylelint-a11y": "1.2.1",
		"stylelint-config-prettier": "5.2.0",
		"stylelint-config-standard": "18.3.0",
		"stylelint-declaration-block-no-ignored-properties": "2.1.0",
		"stylelint-order": "3.0.1",
		"stylelint-scss": "3.9.3",
		"stylelint-use-nesting": "1.2.1"
	}
}
